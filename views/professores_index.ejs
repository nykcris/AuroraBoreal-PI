<body>

    <!-- ======= Header ======= -->
    <header id="header" class="header fixed-top d-flex align-items-center">

        <div class="d-flex align-items-center justify-content-between">
            <a href="/" class="logo d-flex align-items-center">
                <img src="img/AURORALOGOSFUNDO.ico" alt="">
                <span class="d-none d-lg-block">Aurora Boreal</span>
            </a>
            <i class="bi bi-list toggle-sidebar-btn"></i>
        </div><!-- End Logo -->




        </ul>
        </nav><!-- End Icons Navigation -->

    </header><!-- End Header -->

    <main id="main" class="main">

        <div class="pagetitle">
            <h1>Painéis e gerenciamentos</h1>
            <nav>
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="/">Pág.inicial</a></li>
                    <li class="breadcrumb-item active">Painéis</li>
                </ol>
            </nav>
        </div><!-- End Page Title -->

        <section class="section dashboard">
            <div class="row">

                <!-- Left side columns -->
                <div class="col-lg-8">
                    <div class="row">

                       

                    

                       

                           
                        <!-- Pagamentos Card -->
                        <div class="col-xxl-4 col-xl-12">

                            <div class="card info-card customers-card">


                                <div class="card-body">
                                    <h5 class="card-title">Salário Atual <span>| Pagamento dia 05</span></h5>

                                    <div class="d-flex align-items-center">
                                        <div
                                            class="card-icon rounded-circle d-flex align-items-center justify-content-center">
                                            <i class="bi bi-cash" style="color: red;"></i>
                                        </div>
                                        <div class="ps-3">
                                            <h6>R$ 2.800,00</h6>


                                        </div>
                                    </div>

                                </div>
                            </div>

                        </div><!-- End Customers Card -->

                        
                           
                        <!-- Disciplinas Card -->
                        <div class="col-xxl-4 col-xl-12">

                            <div class="card info-card customers-card">


                                <div class="card-body">
                                    <h5 class="card-title"> Turmas <span>| Disciplinas</span></h5>

                                    <div class="d-flex align-items-center">
                                        <div
                                            class="card-icon rounded-circle d-flex align-items-center justify-content-center">
                                            <i class="bi bi-people"></i> 
                                        </div>
                                        <div class="ps-3">
                                            <h6>1 A- Química</h6>
                                            <h6>1 B- Física</h6>
                                            <h6>1 B- Química</h6>

                                        </div>
                                    </div>

                                </div>
                            </div>

                        </div><!-- End Disciplinas Card -->

                      


                    </div>
                </div><!-- End Left side columns -->

                <!-- Right side columns -->
                <div class="col-lg-4">
                    <!-- Multi Columns Form -->
                    <div class="card">
                        <div class="card-body">
                            <h5 class="card-title">Registro de Uso De Material</h5>

                            <form class="row g-3">
                                <div class="col-md-12">
                                    <label for="inputName5" class="form-label">Nome do Material:</label>
                                    <input type="text" class="form-control" id="inputName5">
                                </div>
                                
                                <div class="col-md-6">
                                    <label for="quantidade" class="form-label">Quantidade: </label>
                                    <input type="number" class="form-control" id="quantidade">
                                </div>
                        </div>

                     


                        <div class="text-center">
                            <button type="submit" class="btn btn-primary">Enviar</button>
                            <button type="reset" class="btn btn-secondary">Limpar</button>
                        </div>
                        </form>
                    </div>
                </div><!-- End Multi Columns Form -->
                <!-- End Listar Alunos -->
                <div class="col-12">
                    <div class="card recent-sales overflow-auto">
                    <div class="card-body">
                        <h5 class="card-title">Lista de Alunos<span></span></h5>
    
                        <table class="table table-borderless datatable">
                        <thead>
                            <tr>
                            <th scope="col">ID</th>
                            <th scope="col">Nome</th>
                            <th scope="col">Email</th>
                            <th scope="col">Status</th>
                            <th scope="col">Perfil</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% if (typeof rows != 'undefined') { %>
                            <% for( let index = 0; index < rows.length; index++ ) { %>
                                <tr><%  %>
                                <th scope="row">#<%= rows[index].id_user %></th>
                                <td><%= rows[index].name_user %></td>
                                <td><%= rows[index].email_user %></td>
                                <% if (rows[index].status_user == 1) { %>
                                    <td><span class="badge bg-success">Ativo</span></td>
                                <% } else if (rows[index].status_user == 0) { %>
                                    <td><span class="badge bg-secondary"></span></td>
                                <% } %>
                                <td><%= rows[index].id_perfil %></td>
                                </tr><%  %>
                            <% } %>
                            <% } %>
                        </tbody>
                        </table>
    
                    </div>
    
                    </div>
                </div>
                <!-- Table with stripped rows -->

                
                          <!-- Formulário para adicionar aluno à turma -->
                          <div class="card">
                            <div class="card-body">
                                <h5 class="card-title">
                                    <label for="turma-select" class="form-label">chamada - Selecione a Turma:</label>
                                    <select class="form-select" id="turma-select" name="turma">
                                        <option value="" selected disabled>Selecione a turma</option>
                                        <option value="1A">1° A- Química</option>
                                        <option value="1B">1° B-Física</option>
                                        <option value="1C">1° B- Química</option>
                                    </select>
                                </h5>
                        
                                <h5 class="card-title mt-4">Data da chamada</h5>
                                <form id="chamada-form">
                                    <div class="mb-3">
                                        <input type="date" class="form-control" id="data-chamada" name="data-chamada" required>
                                    </div>
                                    <button type="button" id="fazer-chamada" class="btn btn-primary">Fazer chamada</button>
                                </form>
                        
                                <table class="table table-striped">
                                    <thead>
                                        <tr>
                                            <th scope="col">#</th>
                                            <th scope="col">Nome</th>
                                            <th scope="col">Presença</th>
                                        </tr>
                                    </thead>
                                    <tbody id="alunos-tbody">
                                        <tr>
                                            <th scope="row">1</th>
                                            <td>Brandon Jacob</td>
                                            <td class="presenca-coluna"></td>
                                        </tr>
                                        <tr>
                                            <th scope="row">2</th>
                                            <td>Bridie Kessler</td>
                                            <td class="presenca-coluna"></td>
                                        </tr>
                                        <tr>
                                            <th scope="row">3</th>
                                            <td>Ashleigh Langosh</td>
                                            <td class="presenca-coluna"></td>
                                        </tr>
                                        <tr>
                                            <th scope="row">4</th>
                                            <td>Angus Grady</td>
                                            <td class="presenca-coluna"></td>
                                        </tr>
                                        <tr>
                                            <th scope="row">5</th>
                                            <td>Raheem Lehner</td>
                                            <td class="presenca-coluna"></td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <!--fim do formulário-->
                <!-- Formulário disciplinas-diário de classe  -->

                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">
                            <label for="turma-select" class="form-label">Diário de classe - Selecione a Turma:</label>
                            <select class="form-select" id="turma-select" name="turma">
                                <option value="" selected disabled>Selecione a turma</option>
                                <option value="1A">1° A -Química</option>
                                <option value="1B">1° B - Física </option>
                                <option value="1C">1° B- Química</option>
                               
                            </select>
                        </h5>
                        <form action="" method="post">
                           
                            <div class="mb-3">
                                <label class="form-label">Data da aula:</label>
                                <input type="date" class="form-control" id="data-aula" name="data-aula" required>
                            </div>
                           
                            <div class="col-md-6">
                                <label for="descricao" class="form-label">Descrição da aula: </label>
                                <input type="text" class="form-control" id="descricao">
                            </div>
                            <br>
                            <button type="submit" class="btn btn-primary">Adicionar Registro de Aula</button>
                        </form>
                        <!-- Fim do formulário -->
                    </div>
                </div>

                <!-- Table Notas  -->

                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">
                            <label for="turma-select" class="form-label">Notas - Selecione a Turma:</label>
                            <select class="form-select" id="turma-select" name="turma">
                                <option value="" selected disabled>Selecione a turma</option>
                                <option value="1A">1° A- Química</option>
                                <option value="1B">1° B- Física</option>
                                <option value="1C">1° B- Química</option>
                            </select>
                        </h5>
                
                        <table class="table table-borderless datatable">
                            <thead>
                                <tr>
                                    <th scope="col">#</th>
                                    <th scope="col">Aluno</th>
                                    <th scope="col">Notas das Atividades</th>
                                    <th scope="col">Média da Nota</th>
                                    <th scope="col">Status</th>
                                </tr>
                            </thead>
                            <tbody id="tabela-notas">
                                <tr id="aluno-1">
                                    <th scope="row">1</th>
                                    <td>Brandon Jacob</td>
                                    <td id="atividades-1"></td>
                                    <td><span id="media-1">-</span></td>
                                    <td><span id="status-1" class="badge">-</span></td>
                                </tr>
                                <tr id="aluno-2">
                                    <th scope="row">2</th>
                                    <td>Barbie Kids</td>
                                    <td id="atividades-2"></td>
                                    <td><span id="media-2">-</span></td>
                                    <td><span id="status-2" class="badge">-</span></td>
                                </tr>
                                <tr id="aluno-3">
                                    <th scope="row">3</th>
                                    <td>Day Limns</td>
                                    <td id="atividades-3"></td>
                                    <td><span id="media-3">-</span></td>
                                    <td><span id="status-3" class="badge">-</span></td>
                                </tr>
                                <!-- Adicione mais linhas para cada aluno -->
                            </tbody>
                        </table>
                
                        <!-- Formulário para adicionar notas -->
                        <div class="mt-4">
                            <h5 class="card-title">Adicionar Nota</h5>
                            <form id="form-adicionar-nota" onsubmit="adicionarNota(event)">
                                <div class="mb-3">
                                    <label for="aluno-select" class="form-label">Selecione o Aluno:</label>
                                    <select class="form-select" id="aluno-select" required>
                                        <option value="1">Brandon Jacob</option>
                                        <option value="2">Barbie Kids</option>
                                        <option value="3">Day Limns</option>
                                        <!-- Adicione mais alunos com IDs únicos -->
                                    </select>
                                </div>
                                <div class="mb-3">
                                    <label for="nota-input" class="form-label">Nota da Atividade:</label>
                                    <input type="number" class="form-control" id="nota-input" min="0" max="10" step="0.1" required>
                                </div>
                                <button type="submit" class="btn btn-primary">Adicionar Nota</button>
                            </form>
                        </div>
                    </div>
                </div>
                <!--fim da tabela de notas-->                

            </div><!-- End Right side columns -->
            </div>
        </section>

    </main><!-- End #main -->

